<Window x:Class="subscribers_box.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:subscribers_box"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        Title="Subscribers Box" Width="250" Height="150">
  <Grid>
    <Label Name="Subscribers" FontSize="32" HorizontalAlignment="Center" VerticalAlignment="Center" Padding="0,0,0,16"/>
    <StackPanel Name="Start">
      <Label Content="Channel URL" HorizontalAlignment="Center" Padding="0"/>
      <TextBox Name="ChannelURL"/>
      <Button Content="Go" Click="Go_Click"/>
    </StackPanel>
  </Grid>
</Window>
